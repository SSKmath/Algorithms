#include <iostream>
#include <vector>

using namespace std;

vector<int> bubbleSort(vector<int> A)
{
	int t;
	for (int end = A.size(); end > 1; end--)
	{
		for (int i = 1; i < end; i++)
		{
			if (A[i - 1] > A[i])
			{
				t = A[i - 1];
				A[i - 1] = A[i];
				A[i] = t;
			}
		}
	}
	return A;
}

int main()
{
	vector<int> A = {1, 2, 3, 9, 7, 4, 4, 2};
	for (int i = 0; i < A.size(); i++)
	{
		cout << A[i] << ' ';
	}
	cout << endl;
	A = bubbleSort(A);
	for (int i = 0; i < A.size(); i++)
	{
		cout << A[i] << ' ';
	}
}
